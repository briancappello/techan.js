{
  "name": "techanjs",
  "version": "0.10.0",
  "description": "A visual, technical analysis and charting (Candlestick, OHLC, indicators) library built on D3.",
  "keywords": [
    "technical analysis",
    "financial",
    "finance",
    "visualization",
    "chart",
    "svg",
    "candlestick",
    "ohlc",
    "indicators",
    "volume"
  ],
  "homepage": "techanjs.org",
  "author": {
    "name": "Andr√© Dumas",
    "url": "http://andredumas.id.au"
  },
  "jsnext:main": "src/techan.js",
