      "import": "./dist/techan.mjs",
      "require": "./dist/techan.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andredumas/techan.js.git"
  },
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "d3": "^7.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "jsdom": "^22.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.js"
